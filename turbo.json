{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "outputs": []
    },
    "db:build": {
      "cache": false,
      "outputs": [
        "node_modules/.prisma/**"
      ]
    },
    "build": {
      "dependsOn": [
        "^db:build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": []
    }
  }
}
