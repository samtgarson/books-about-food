{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "start": {
      "outputs": []
    },
    "@books-about-food/database#build": {
      "env": ["DATABASE_URL", "DATABASE_DIRECT_URL"],
      "outputs": ["generated/**"]
    },
    "build": {
      "dependsOn": ["@books-about-food/database#build"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": []
    }
  }
}
