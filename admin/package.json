{
  "name": "@books-about-food/admin",
  "version": "0.1.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon -r dotenv/config --exec node -r ts-node/register ./lib/index.ts",
    "debug": "nodemon -r dotenv/config --exec node -r ts-node/register --inspect-brk ./lib/index.ts",
    "build": "npx esbuild ./lib/index.ts --bundle --outfile=./dist/index.js --platform=node --external:fastify --external:pg-hstore --external:pg-native --external:@prisma/client --external:sharp --external:uglify-js --loader:.node=file --target=node20"
  },
  "dependencies": {
    "@books-about-food/core": "*",
    "@books-about-food/database": "*",
    "@books-about-food/jobs": "*",
    "@books-about-food/shared": "*",
    "@forestadmin/agent": "^1.36.0",
    "@forestadmin/datasource-sql": "^1.7.39",
    "@koa/bodyparser": "^5.0.0",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.1",
    "async-parallel": "^1.2.3",
    "buffer-image-size": "^0.6.4",
    "color": "^4.2.3",
    "inngest": "^3.6.0",
    "koa": "^2.14.2",
    "koa-jwt": "^4.0.4",
    "pg": "^8.11.3",
    "pg-connection-string": "^2.6.2",
    "pkg-dir": "^5.0.0"
  },
  "devDependencies": {
    "@types/color": "^3.0.5",
    "@types/express": "^4.17.20",
    "@types/koa": "^2.13.10",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.3",
    "@types/pg": "^8.10.7",
    "@types/sequelize": "^4.28.17",
    "dotenv": "^16.3.1",
    "esbuild": "^0.16.12",
    "forest-cli": "^4.2.2",
    "nodemon": "^3.0.1"
  }
