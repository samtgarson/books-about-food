{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "main": "index.ts",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon -r dotenv/config --exec node -r ts-node/register ./lib/index.ts",
    "debug": "nodemon -r dotenv/config --exec node -r ts-node/register --inspect-brk ./lib/index.ts",
    "build": "npx esbuild ./lib/index.ts --bundle --outfile=./dist/index.js --platform=node --external:fastify --external:pg-hstore --external:pg-native --external:@prisma/client --external:sharp --target=node18"
  },
  "dependencies": {
    "@forestadmin/agent": "^1.6.4",
    "@forestadmin/datasource-sql": "^1.1.13",
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "async-parallel": "^1.2.3",
    "buffer-image-size": "^0.6.4",
    "database": "*",
    "koa": "^2.14.1",
    "koa-jwt": "^4.0.4",
    "pg": "^8.8.0",
    "pg-connection-string": "^2.5.0",
    "pkg-dir": "^5.0.0",
    "shared": "*"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/koa": "^2.13.5",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "@types/pg": "^8.6.6",
    "@types/sequelize": "^4.28.14",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.12",
    "forest-cli": "^3.0.1-beta.1",
    "nodemon": "^2.0.20"
  }
